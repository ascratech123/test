.save_button.mdl-card.mdl-shadow--1dp.agendaheight.p-b-10 
  %form.ng-pristine.ng-valid{:action => "#"}
    .mdl-grid
      .mdl-cell--12-col.mdl-cell--12-col-tablet.ml-color--shades-white.m-8
        %h4.heading-border.p-t-10
          = show_field_newQuestion(" ", obj.question)
      = show_field_new_for_poll("1 :", "1",obj) if obj.option_type == "Rating"
      = show_field_new_for_poll("2 :", "2",obj) if obj.option_type == "Rating"
      = show_field_new_for_poll("3 :", "3",obj) if obj.option_type == "Rating"
      = show_field_new_for_poll("4 :", "4",obj) if obj.option_type == "Rating"
      = show_field_new_for_poll("5 :", "5",obj) if obj.option_type == "Rating"
      = show_field_new_for_poll("#{obj.option1} :", "option1",obj) if obj.option1.present?
      = show_field_new_for_poll("#{obj.option2} :", "option2",obj) if obj.option2.present?
      = show_field_new_for_poll("#{obj.option3} :", "option3",obj) if obj.option3.present?
      = show_field_new_for_poll("#{obj.option4} :", "option4",obj) if obj.option4.present?
      = show_field_new_for_poll("#{obj.option5} :", "option5",obj) if obj.option5.present?
      = show_field_new_for_poll("#{obj.option6} :", "option6",obj) if obj.option6.present?
      = show_field_new_for_poll("#{obj.option7} :", "option7",obj) if obj.option7.present?
      = show_field_new_for_poll("#{obj.option8} :", "option8",obj) if obj.option8.present?
      = show_field_new_for_poll("#{obj.option9} :", "option9",obj) if obj.option9.present?
      = show_field_new_for_poll("#{obj.option010} :", "option010",obj) if obj.option010.present?
















-if false
  %form.ng-pristine.ng-valid{:action => "#"}
    .mdl-grid
      = show_field("Question :", obj.question)
      = show_field("Option1:", obj.option1)
      = show_field("Option2 :", obj.option2)
      = show_field("Option3 :", obj.option3)
      = show_field("Option4 :", obj.option4)
      = show_field("Option5 :", obj.option5)
      / = show_field("Option6 :", obj.option6)
    .m-l-18
      .mdl-cell--12-col.mdl-cell--12-col-tablet
        = back_button_detailed_page(:back)  
  = render :partial => 'polls_answers', :locals => {:obj => obj}