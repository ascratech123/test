      - if show_card_except_event_features("custom_page4s")
        = render :partial => "admin/events/cards", :locals => {:icon => 'b_&_w', :img =>"custom_b.png", :objs => @event.custom_page4s, :url => "custom_page4s"}
      
      - if show_card_except_event_features("custom_page5s")
        = render :partial => "admin/events/cards", :locals => {:icon => 'b_&_w', :img =>"custom_b.png", :objs => @event.custom_page5s, :url => "custom_page5s"}        

      - if show_card_except_event_features("my_travels")
        = render :partial => "admin/events/cards", :locals => {:icon => 'b_&_w', :img =>"travel.png", :objs => @event.my_travels, :url => "my_travels"}   

      - if show_card_except_event_features("courses")
        = render :partial => "admin/events/cards", :locals => {:icon => 'b_&_w', :img =>"travel.png", :objs => @event.courses, :url => "courses"}           