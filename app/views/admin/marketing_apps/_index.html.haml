%table.table.mdl-data-table.fullwidth.ml-table-striped.ml-table-bordered.sequenceInputWidht{"template-pagination" => "custom/pager"}
  %tr
    %th Event Name
    %th 
  - objs.each_with_index do |marketing_app,index|
    %tr
      - if marketing_app.mobile_application_id.present?
        %td.font16= link_to marketing_app.event_name, admin_event_mobile_application_path(:event_id => marketing_app.id, :id => marketing_app.mobile_application_id)
      -else
        %td.font16= marketing_app.event_name
      %td.IconDrp
        =# more_vert_button(marketing_app.id)
        %ul.mdl-menu.mdl-menu--bottom-right.mdl-js-menu.mdl-js-ripple-effect{:for => "demo-menu-lower-right#{marketing_app.id}"}
          =# edit_link(params, marketing_app.id)
          =# delete_link(params, marketing_app.id)
= will_paginate objs