%html
  %head
    %title Hobnob
    %meta{:content => "width=device-width, initial-scale=1, maximum-scale=1", :name => "viewport"}
    %script{:defer => "defer", :src => "http://platform.instagram.com/en_US/embeds.js"}
    /= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true
    /= javascript_include_tag 'application', 'data-turbolinks-track' => true
    = csrf_meta_tags
  %body
    <div id="fb-root"></div>
    <script>(function(d, s, id) { var js, fjs = d.getElementsByTagName(s)[0]; if (d.getElementById(id)) return; js = d.createElement(s); js.id = id; js.src = "//connect.facebook.net/en_US/sdk.js#xfbml=1&version=v2.8&appId=206864562711504"; fjs.parentNode.insertBefore(js, fjs); }(document, 'script', 'facebook-jssdk'));</script>
    - if @event.id == 168
      #vmware_logo{:style=>"background:none;background-size:cover;background-repeat: no-repeat;"}
        #load-social-feed
        #contents-social-feed
          .vmware_bg#my-posts{:style => "background:none"}
            = render :partial =>"social_feeds", :locals => {:social_feeds => @social_feeds}
          .centerAlign#loadingText_activity
          #feed_data_social
            = link_to "", "javascript:void(0);",:class => "load_products"
          #social_event_id{:for => params[:event_id].to_i}
    -else
      #load-social-feed
      #contents-social-feed
        #my-posts{:style => "background:none"}
          = render :partial =>"social_feeds", :locals => {:social_feeds => @social_feeds}
        .centerAlign#loadingText_activity
        #feed_data_social
          = link_to "", "javascript:void(0);",:class => "load_products"
        #social_event_id{:for => params[:event_id].to_i}      